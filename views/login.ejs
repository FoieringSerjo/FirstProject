<%- include('partials/header') %>

<div class="container mt-5">
    <a href="/" class="btn btn-lg btn-primary">Go Back</a>
    <h1>Login Here</h1>
    <form action="/login/logindata" method="POST" autocomplete="off"> 
      <div class="mb-3">
        <label  class="username">Username</label>
        <input type="username" name="username" placeholder="Enter Your Username" class="form-control" id="username" >
      </div>

      <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Password</label>
        <input type="password" placeholder="Enter Your Password" name="password" class="form-control" id="password" >
      </div>
      <button type="submit" class="btn btn-primary">Login</button>
    </form>
    
  </div>